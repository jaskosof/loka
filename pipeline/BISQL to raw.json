{
	"name": "BISQL to raw",
	"properties": {
		"activities": [
			{
				"name": "Lookup previous load mark",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "select * from [etl].[IncrementalLoadMark] WHERE SourceServer = 'BISQL'",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DW_SQL",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Load tables",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Lookup previous load mark",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Lookup previous load mark').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Lookup new load mark",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": {
										"value": "select MAX(@{item().MarkColumn}) as MaxMark from @{item().SchemaName}.@{item().TableName}",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"dataset": {
									"referenceName": "DW_SQL",
									"type": "DatasetReference"
								},
								"firstRowOnly": true
							}
						},
						{
							"name": "NewRecords",
							"type": "Switch",
							"dependsOn": [
								{
									"activity": "Lookup new load mark",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"on": {
									"value": "@if(greaterOrEquals(sub(int(activity('Lookup new load mark').output.firstRow.MaxMark), int(item().MarkValue)), 1000000), 'Dynamic load', if(greater(sub(int(activity('Lookup new load mark').output.firstRow.MaxMark), int(item().MarkValue)), 0), 'Diff load', 'Default'))",
									"type": "Expression"
								},
								"cases": [
									{
										"value": "Dynamic load",
										"activities": [
											{
												"name": "Copy data to the landing zone with dynamic range",
												"type": "Copy",
												"dependsOn": [],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "SqlServerSource",
														"sqlReaderQuery": {
															"value": "select * from @{item().SchemaName}.@{item().TableName} with (nolock) where @{item().MarkColumn} > @{item().MarkValue} and @{item().MarkColumn} <= @{activity('Lookup new load mark').output.firstRow.MaxMark} and ?AdfDynamicRangePartitionCondition",
															"type": "Expression"
														},
														"queryTimeout": "02:00:00",
														"partitionOption": "DynamicRange",
														"partitionSettings": {
															"partitionColumnName": {
																"value": "@{item().MarkColumn}",
																"type": "Expression"
															},
															"partitionUpperBound": "",
															"partitionLowerBound": ""
														}
													},
													"sink": {
														"type": "JsonSink",
														"storeSettings": {
															"type": "AzureBlobFSWriteSettings"
														},
														"formatSettings": {
															"type": "JsonWriteSettings"
														}
													},
													"enableStaging": false
												},
												"inputs": [
													{
														"referenceName": "DW_SQL",
														"type": "DatasetReference"
													}
												],
												"outputs": [
													{
														"referenceName": "CMS_V2_DL_Landing_folder",
														"type": "DatasetReference",
														"parameters": {
															"FolderName": {
																"value": "@item().FolderName",
																"type": "Expression"
															}
														}
													}
												]
											},
											{
												"name": "Update Mark with latest value dynamic",
												"type": "SqlServerStoredProcedure",
												"dependsOn": [
													{
														"activity": "Copy data to the landing zone with dynamic range",
														"dependencyConditions": [
															"Succeeded"
														]
													}
												],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"storedProcedureName": "[etl].[usp_IncrementalLoadMark_Update_MarkValue]",
													"storedProcedureParameters": {
														"ID": {
															"value": {
																"value": "@item().ID",
																"type": "Expression"
															},
															"type": "Int32"
														},
														"MarkValue": {
															"value": {
																"value": "@activity('Lookup new load mark').output.firstRow.MaxMark",
																"type": "Expression"
															},
															"type": "String"
														}
													}
												},
												"linkedServiceName": {
													"referenceName": "BIServer_EDW",
													"type": "LinkedServiceReference"
												}
											}
										]
									},
									{
										"value": "Diff load",
										"activities": [
											{
												"name": "Copy data to the landing zone",
												"type": "Copy",
												"dependsOn": [],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "SqlServerSource",
														"sqlReaderQuery": {
															"value": "select * from @{item().SchemaName}.@{item().TableName} with (nolock) where @{item().MarkColumn} > @{item().MarkValue} and @{item().MarkColumn} <= @{activity('Lookup new load mark').output.firstRow.MaxMark}",
															"type": "Expression"
														},
														"queryTimeout": "02:00:00",
														"partitionOption": "None"
													},
													"sink": {
														"type": "JsonSink",
														"storeSettings": {
															"type": "AzureBlobFSWriteSettings"
														},
														"formatSettings": {
															"type": "JsonWriteSettings"
														}
													},
													"enableStaging": false
												},
												"inputs": [
													{
														"referenceName": "DW_SQL",
														"type": "DatasetReference"
													}
												],
												"outputs": [
													{
														"referenceName": "CMS_V2_DL_Landing_file",
														"type": "DatasetReference",
														"parameters": {
															"TableName": {
																"value": "@replace(replace(item().TableName, '[', ''), ']', '')",
																"type": "Expression"
															},
															"PipelineID": {
																"value": "@pipeline().RunId",
																"type": "Expression"
															},
															"FolderName": {
																"value": "@item().FolderName",
																"type": "Expression"
															}
														}
													}
												]
											},
											{
												"name": "Update Mark with latest value",
												"type": "SqlServerStoredProcedure",
												"dependsOn": [
													{
														"activity": "Copy data to the landing zone",
														"dependencyConditions": [
															"Succeeded"
														]
													}
												],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"storedProcedureName": "[etl].[usp_IncrementalLoadMark_Update_MarkValue]",
													"storedProcedureParameters": {
														"ID": {
															"value": {
																"value": "@item().ID",
																"type": "Expression"
															},
															"type": "Int32"
														},
														"MarkValue": {
															"value": {
																"value": "@activity('Lookup new load mark').output.firstRow.MaxMark",
																"type": "Expression"
															},
															"type": "String"
														}
													}
												},
												"linkedServiceName": {
													"referenceName": "BIServer_EDW",
													"type": "LinkedServiceReference"
												}
											}
										]
									}
								]
							}
						}
					]
				}
			}
		],
		"folder": {
			"name": "Facts"
		},
		"annotations": [],
		"lastPublishTime": "2021-11-22T11:34:42Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}